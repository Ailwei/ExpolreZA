{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "moduleResolution": "bundler", 
    "esModuleInterop": true,
    "resolveJsonModule": true,
    "skipLibCheck": true,
    "jsx": "react-jsx",
    "customConditions": ["react-native"],
    "types": [],
    "lib": ["ES2015", "DOM"] 
  },
  "include": ["**/*"],
  "exclude": ["node_modules", "node_modules/expo/**"]
}
